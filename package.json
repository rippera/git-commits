{
  "name": "git-commits",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/rippera/git-commits.git",
  "author": "rippera <mikehil.mezurnishvili10b@geolab.edu.ge>",
  "license": "MIT",
  "dependencies": {},
  "scripts":{
    "cz":"./node_modules/.bin/cz"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": false,
        "disableSubjectLowerCase": false,
        "maxHeaderWidth": 50,
        "maxLineWidth": 50,
        "defaultScope": "APP",
        "defaultSubject": "Comment from PC",
        "defaultBody": "This is default body",
        "defaultIssues": "Project issue"
    }
  }
}
